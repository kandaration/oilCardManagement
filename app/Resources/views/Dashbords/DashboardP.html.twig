    <script>
        $(document).ready(function () {

            $.ajax({
                type: "POST",
                url: "{{ path('reportingC_P_CarteDepenceStat') }}",
                cache: false,
                success: function (rep) {
                    console.log(rep);
                    var year = moment(new Date().getYear() + 1900, 'YYYY').format('YYYY');
                    var Depence = jQuery.parseJSON(rep);
                    var data = [];
                    var cat = [];
                    for (var i=1;i<=31;i++)
                        cat.push(i);
                    console.log(data);
                    for (var key in Depence) {

                      /*  if (key != "all") {
                            if (TopClt[key] != 0) {
                                data.push([key, TopClt[key]]);
                            }*/
                      var DepencePerDay=[];
                      for(var keyday in Depence[key])
                      {
                          DepencePerDay.push(Depence[key][keyday])

                      }
                      if(moment(key, 'MM').format('MM')== moment(new Date().getMonth() + 1, 'MM').format('MM') )
                        data.push({name: moment(key, 'MM').format('MMM'),data:DepencePerDay});
                         else
                          data.push({name: moment(key, 'MM').format('MMM'),data:DepencePerDay,visible: false});

                        //data.append() "['" + key + "'," + StationStat[key] + "],";
                        /*}
                        else {
                            var ca = TopClt[key];
                        }*/
                    }
                    var options = {

                        title: {
                            text: 'Comparateur dépence mensulle '+ year
                        },

                        yAxis: {
                            title: {
                                text: 'Montant (DT)'
                            }
                        },
                        legend: {
                            layout: 'vertical',
                            align: 'right',
                            verticalAlign: 'middle'
                        },

                        xAxis: {
                            categories: cat
                        },

                        series: data

                    };
                    Highcharts.chart('container', options);
                    $(".sk-folding-cube").hide();
                }
            });



        });


    </script>

