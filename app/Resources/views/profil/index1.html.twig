<script>
    function update(nom,description,id) {

        document.getElementById("Tprofil").value=nom;
        document.getElementById("Tdesc").value=description;

        document.getElementById("xxx").setAttribute("action","/CartePetroliere/web/app_dev.php/profil/"+id+"/edit");
    }
    function form_submitadd() {
        if($("#Tprofiladd").val()=="")
        {
            alert("profil est obligatoire");
        }
        else if($("#Tdescadd").val()=="")
        {
            alert("description est obligatoire");
        }
        else
        { document.getElementById("xxx").submit();}
    }
    function form_submitmodif() {
        if($("#Tprofil").val()=="")
        {
            alert("profil est obligatoire");
        }
        else if($("#Tdesc").val()=="")
        {
            alert("description est obligatoire");
        }
        else
        { document.getElementById("xxx").submit();}
    }
</script>

