<?php

namespace GestionAccesBundle\Entity;

/**
 * ProfilUtilisateur
 */
class ProfilUtilisateur
{
    /**
     * @var integer
     */
    private $id_PUser;

    /**
     * @var \GestionAccesBundle\Entity\Profil
     */
    private $profil;

    /**
     * @var \GestionAccesBundle\Entity\Utilisateur
     */
    private $utilisateur;


    /**
     * Get idPUser
     *
     * @return integer
     */
    public function getIdPUser()
    {
        return $this->id_PUser;
    }

    /**
     * Set profil
     *
     * @param \GestionAccesBundle\Entity\Profil $profil
     *
     * @return ProfilUtilisateur
     */
    public function setProfil(\GestionAccesBundle\Entity\Profil $profil = null)
    {
        $this->profil = $profil;

        return $this;
    }

    /**
     * Get profil
     *
     * @return \GestionAccesBundle\Entity\Profil
     */
    public function getProfil()
    {
        return $this->profil;
    }

    /**
     * Set utilisateur
     *
     * @param \GestionAccesBundle\Entity\Utilisateur $utilisateur
     *
     * @return ProfilUtilisateur
     */
    public function setUtilisateur(\GestionAccesBundle\Entity\Utilisateur $utilisateur = null)
    {
        $this->utilisateur = $utilisateur;

        return $this;
    }

    /**
     * Get utilisateur
     *
     * @return \GestionAccesBundle\Entity\Utilisateur
     */
    public function getUtilisateur()
    {
        return $this->utilisateur;
    }
}
